<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.itwillbs.de.approval.mapper.ApprovalMapper">
	<select id="selectEmployeeInfo" parameterType="String"
								resultType="kr.co.itwillbs.de.approval.dto.DraftDTO">
		select *
		from employee_test
			<where>
				<if test="userId != null">AND id = #{userId} </if>
			</where>
	</select>
	
</mapper>